{
  "name": "azkar",
  "displayName": "Azkar",
  "publisher": "ZiadHussein",
  "icon": "assets/logo.png",
  "description": "Have Islamic Azkar right in your VS Code status bar",
  "repository": {
    "type": "git",
    "url": "https://github.com/ziadh/azkar_vscextension.git"
  },
  "version": "1.0.9",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.toggleSide",
        "title": "Azkar: Toggle StatusBar Side"
      },
      {
        "command": "extension.toggleLanguage",
        "title": "Azkar: Toggle Language"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}